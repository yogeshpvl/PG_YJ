generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Sample model for guest/owner
model User {
  id        Int      @id @default(autoincrement())
  name      String
  phone     String   @unique
  role      String   // 'guest' or 'owner'
  createdAt DateTime @default(now())
}
